{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "flowtype"],
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true, "optionalDependencies": false, "peerDependencies": false }],
    "no-void": 0,
    "global-require": 0,
    "react/jsx-filename-extension": [1, { extensions: [".js", ".jsx"] }]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
